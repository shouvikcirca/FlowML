name: tensorflow-example

entry_points:
  tuning:
    command: "python hyperparameter_tuning.py"

  ht:
    parameters:
      experiment_name: {type: string, default: 'default'}
      learning_rate_start: {type: float, default: 0.1}
      learning_rate_end: {type: float, default: 0.3}
      vocab_length_start: {type: float, default: 100.0}
      vocab_length_end: {type: float, default: 110.0}
      seq_padding_style: {type: string, default: 'post'}
      embedding_dim_start: {type:float, default: 100.0}
      embedding_dim_end: {type:float, default: 110.0}
      bs_start: {type: float, default: 64.0}
      bs_end: {type: float, default: 96.0}
      epochs_start: {type: float, default: 5.0}
      epochs_end: {type: float, default: 10.0}
    command: "python baseRunnerHp.py \
      --experiment-name {experiment_name}
      --learning-rate-start {learning_rate_start} \
      --learning-rate-end {learning_rate_end} \
      --vocab-length-start {vocab_length_start} \
      --vocab-length-end {vocab_length_end} \
      --seq-padding-style {seq_padding_style} \
      --embedding-dim-start {embedding_dim_start} \
      --embedding-dim-end {embedding_dim_end} \
      --bs-start {bs_start} \
      --bs-end {bs_end} \
      --epochs-start {epochs_start} \
      --epochs-end {epochs_end}" 

